let len: 10; 
	a[!len]: 0;
	proc getFactArr(n)
		let result[!n]: 0;
			temp: 0;
			nn: 0;
			rec proc fac(x) 
				if =(!x,0) then return 1  
				else 
					x := -(!x,1);
					return *(+(!x,1),fac(x))
				fi
		in
			nn := !n;
			while <>(!nn,0)
			do
				temp := !nn;
				nn := -(!nn,1);
				result[!nn] := fac(temp)
			od;
			return result
		end
in
	a := getFactArr(len);
	call printArray(a)
end